<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rock_boy69.car_parking.DeleteUsersActivity">

    <TextView
        android:id="@+id/ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="36dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/profile_display_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/ID"
        android:layout_below="@+id/ID"
        android:layout_marginTop="23dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/user_hora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/profile_display_name"
        android:layout_below="@+id/profile_display_name"
        android:layout_marginTop="31dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/user_local"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/user_hora"
        android:layout_below="@+id/user_hora"
        android:layout_marginTop="33dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/user_lugar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/user_local"
        android:layout_below="@+id/user_local"
        android:layout_marginTop="36dp"
        android:text="TextView" />
</RelativeLayout>
